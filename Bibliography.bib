@article{Hardymon-1983fk,
	abstract = {The argument has an appealing logic to it: because investments in new technologies and new lines of business are inherently risky, why not hedge them by buying some degree of participation through a venture capital operation? If things go well, the external venture can always be acquired; if they go poorly or if plans change, investors can always sell out---ideally with a profit or at least with an enhanced understanding of once unfamiliar technologies, products, and markets. By using the venture capital approach, companies looking to diversify need not risk valuable management time or attention. They can await results and then, when matters are clearer, choose to become more directly involved. So runs the argument. The evidence, however, points in the opposite direction. In practice, as the authors show, the very factors that make for a successful venture capital operation compromise its ability to serve the ends of diversification. [ABSTRACT FROM AUTHOR]},
	author = {Hardymon, G. Felda and DeNino, Mark J. and Salter, Malcolm S.},
